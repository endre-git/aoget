    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: AOGet",
            "type": "python",
            "request": "launch",
            "program": "aoget/aogetapp.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {"PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/aoget/:${workspaceFolder}/aoget/tests/"}
        },
        {
            "name": "Python: Profile AOGet",
            "type": "python",
            "request": "launch",
            "program": "aoget/aogetapp.py",
            "args": [
                "-m",
                "cProfile",
                "-o",
                "${workspaceFolder}/profile_output.prof"
            ],
            "console": "integratedTerminal",
            "env": {"PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/aoget/:${workspaceFolder}/aoget/tests/"}
        },
    ]
}