{
    "debug": false,
    "database-url": "sqlite:///settings/aoget.db",
    "settings-folder": "settings/",
    "target-folder-history-file": "settings/target-folder-history.lst",
    "url-history-file": "settings/url-history.lst",
    "url-cache-folder": "settings/url-cache",
    "auto-resolve-file-sizes": true,
    "log-file-path": "aoget.log",
    "crash-log-file-path": "aoget-crash-log.html",
    "default-download-folder": "C:\\AoGet",
    "low-bandwidth-limit": 100,
    "medium-bandwidth-limit": 1024,
    "high-bandwidth-limit": 5192,
    "job-autonaming-pattern": "url",
    "job-subfolder-policy": "per-job",
    "per-job-default-thread-count": 10,
    "auto-start-jobs": true,
    "overwrite-existing-files": true,
    "url-cache-enabled": true
}